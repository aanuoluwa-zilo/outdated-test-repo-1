{
  "api": {
    "base_url": "https://api.example.com",
    "timeout": 30,
    "retry_attempts": 3,
    "rate_limit": {
      "requests_per_minute": 60,
      "burst_limit": 10
    }
  },
  "endpoints": {
    "users": "/api/v1/users",
    "products": "/api/v1/products",
    "orders": "/api/v1/orders"
  },
  "authentication": {
    "type": "bearer",
    "header": "Authorization"
  }
}